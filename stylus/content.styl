#content
	background-color #e1e1e1
	padding 50px 0

	ul
		margin 0
		margin-top 50px
		padding 0
		list-style none
		font-size 0

		li
			margin-bottom 50px
			font-size 16px
			text-transform uppercase
			font-weight 300
			width 50%
			display inline-block

			img
				vertical-align middle
				margin-right 15px